package interface_puertiventana;

public class P3 extends Entrances implements Iwiring{
	
	public P3() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	public void abrir() {
		System.out.println("Abriendo/cerrando puerta"); //Placeholder
		super.apertura =! super.apertura;
	}
	
	public void auto_close() {
		System.out.println("Auto-closing..."); //Placeholder
	}
	
	public void estado() {
		if (super.apertura = true)
			System.out.println("Abierto");
		else
			System.out.println("Cerrado");
	}

}
