package interface_puertiventana;

public class J4 extends Entrances implements Iwiring {
	
	
	
	public J4() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	public void abrir() {
		System.out.println("Abriendo/cerrando puerta"); //Placeholder useless
		super.apertura =! super.apertura;
	}
	
	public void auto_close() {
		System.out.println("Performing action"); //Placeholder
	}
	
	public void estado() {
		if (super.apertura = true)
			System.out.println("J4 Abierto");
		else
			System.out.println("J4 Cerrado");
	}

}
