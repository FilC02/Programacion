package interface3_clasesherencia;

public class Principal {
	public Principal() {
		Perro  sus = new Perro("PAAAAA", "PBBBBB");
		Gato sus2 = new Gato("GAAAA", "GBBBB");
		Raton sus3 = new Raton("RAAAA", "RBBBB");
		System.out.println(sus.toString());
		sus.perseguir(sus.tipo);
		
	}
	public static void main(String[] args) {
		Principal amogus = new Principal();
		
		
		
	}

}
