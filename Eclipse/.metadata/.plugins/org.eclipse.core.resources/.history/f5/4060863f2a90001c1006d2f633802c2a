import java.util.*;

public class Principal {
	
	
	Automatico P1 = new Automatico();
	Manual P2 = new Manual();
	Automatico P3 = new Automatico();
	Window w4 = new Window();
	static Scanner sus = new Scanner(System.in);
	
	public Principal() {
		
		
		P1.estado();
		P2.estado();
		P3.estado();
		w4.estado();
	}
	
	
	
	
	
	public static void main(String[] args) {
		new Principal();

	}

}
