package empleados_poli;

public class Mensajeros {
	private int edad;

	public int getEdad() {
		return edad;
	}

	public void setEdad(int edad) {
		this.edad = edad;
	}
	public Mensajeros(String codigo, String nombre, double sueldobase, int antiguedad) {
		super(codigo,nombre,sueldobase);
		this.antiguedad = antiguedad;
	}
}
