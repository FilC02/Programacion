package interface_puertasreviewed;


public class Manual extends Entrances {
	
	public class P1 extends Entrances implements Iwiring{
		public boolean apertura = false;
		
		public P1() {
			super();
			// TODO Auto-generated constructor stub
		}
		
		
		@Override
		public void open() {
			super.apertura =! super.apertura; }
		
		@Override
		public void estado() {
			if (apertura == false) 
				System.out.println("La puerta está abierta"); 
			else 
				System.out.println("La puerta está cerrada");
		}
		
	}

}


