package interface_vehiculo;

public class Camion extends VehiculoMotorizado {
	private int remolques;
	private int pasajeros;

	public Camion() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Camion(String fabricante, String modelo, int año, double kilometraje, int remolques, int pasajeros) {
		super(fabricante, modelo, año, kilometraje);
		this.remolques = remolques;
		this.pasajeros = pasajeros;
		// TODO Auto-generated constructor stub
	}
	
	
	

}
