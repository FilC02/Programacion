package empleadoAbst;

public abstract class Empleados extends Empresa {
	protected String codigo;
	protected String nombre;
	protected double sfijo;
	
	
	
	//public abstract String empresa();
	
	
	public Empleados(String codigo, String nombre, String empresa) {
		this.codigo = codigo;
		this.nombre = nombre;
		empresa = super.denominacion;
		super.totalemp++;

		
	}
	
	
	
	



	public abstract void sueldofi();
	
	
	public abstract double sueldotot();





	@Override
	public String toString() {
		return "Empleados [codigo=" + codigo + ", nombre=" + nombre ;
	}
	
	
	
}
