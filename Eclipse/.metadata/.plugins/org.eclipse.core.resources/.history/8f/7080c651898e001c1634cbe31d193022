package interface_vehiculo;

public class Auto extends VehiculoMotorizado implements IcapacidadLimite {
	private Tipos estilo;
	private int pasajeros;

	public Auto() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Auto(String fabricante, String modelo, int año, double kilometraje, Tipos estilo, int pasajeros) {
		super(fabricante, modelo, año, kilometraje);
		this.estilo = estilo;
		this.pasajeros = pasajeros;
		// TODO Auto-generated constructor stub
	}
	
	public void comprobar() {
		if (estilo.name("VAN") = "VAN") {
			System.out.println("A");
		}
	}
	
	
	
	
	
}
